{
    "name": "ethvm",
    "version": "1.1.0-hotfix.1",
    "description": "EthVM: Open source Ethereum networks explorer",
    "author": "EthVM Team",
    "private": true,
    "scripts": {
        "start:dev": "NODE_ENV=development vue-cli-service serve",
        "build": "vue-cli-service --mode production build",
        "build:staging": "vue-cli-service --mode staging build",
        "build:hash": "ROUTER_MODE=hash yarn build",
        "clean": "rm -rf dist/",
        "depcheck": "depcheck --config=.depcheckrc",
        "lint": "vue-cli-service lint --no-fix",
        "lint:fix": "vue-cli-service lint --fix",
        "apollo": "apollo",
        "apollo:schema": "apollo service:download ./src/apollo/schemas/api.json",
        "apollo:codegen": "yarn apollo:schema && apollo client:codegen --target=typescript  --globalTypesFile=./src/apollo/global/globalTypes.ts  apolloTypes"
    },
    "husky": {
        "hooks": {
            "pre-commit": "bash ./checkBranch.sh && yarn lint:fix",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "dependencies": {
        "@babel/core": "^7.4.5",
        "@commitlint/config-conventional": "^11.0.0",
        "@sentry/browser": "^5.23.0",
        "@sentry/integrations": "^5.23.0",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link": "^1.2.14",
        "apollo-link-error": "^1.1.13",
        "apollo-link-http": "^1.5.17",
        "apollo-link-rest": "^0.7.0",
        "apollo-link-retry": "^2.2.16",
        "apollo-link-ws": "^1.0.20",
        "apollo-utilities": "^1.3.3",
        "bignumber.js": "^9.0.0",
        "canvas-confetti": "^0.1.0",
        "chart.js": "^2.9.3",
        "clipboard-copy": "^4.0.1",
        "commitlint": "^11.0.0",
        "core-js": "^2.6.5",
        "date-fns": "^1.30.1",
        "eslint-plugin-graphql": "^3.1.1",
        "eslint-plugin-vue": "^7.0.1",
        "graphql": "14.6.0",
        "graphql-anywhere": "^4.2.7",
        "graphql-tag": "^2.10.3",
        "husky": "^4.3.0",
        "moment": "^2.27.0",
        "number-formatter": "^1.2.0",
        "prismjs": "^1.26.0",
        "rxjs": "^6.5.5",
        "store": "^2.0.12",
        "subscriptions-transport-ws": "^0.9.16",
        "vue": "^2.6.11",
        "vue-apollo": "^3.0.3",
        "vue-chartjs": "^3.5.0",
        "vue-chartjs-typescript": "^3.3.3",
        "vue-class-component": "^7.2.3",
        "vue-i18n": "^8.17.6",
        "vue-matomo": "^4.1.0",
        "vue-prism-component": "^1.2.0",
        "vue-property-decorator": "^8.4.2",
        "vue-qr": "^1.5.3",
        "vue-router": "^3.1.6",
        "vue-time-ticker": "^0.1.3",
        "vue-timeago": "^5.1.2",
        "vuetify": "^1.5.4",
        "vuetify-loader": "^1.2.2",
        "vuex": "^3.4.0",
        "vuex-class": "^0.3.2",
        "web3": "^1.2.6",
        "web3-utils": "^1.3.0",
        "webpack-bundle-analyzer": "^3.9.0",
        "webpack-merge": "^5.2.0"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-free": "^5.10.1",
        "@types/node": "~14.0.1",
        "@vue/cli-plugin-babel": "^3.12.1",
        "@vue/cli-plugin-eslint": "^3.12.1",
        "@vue/cli-plugin-typescript": "^3.12.1",
        "@vue/cli-service": "^3.12.1",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^3.2.0",
        "apollo": "^2.27.4",
        "css-loader": "^2.1.1",
        "depcheck": "^1.2.0",
        "eslint-plugin-prettier": "^3.1.3",
        "prettier": "2.0.5",
        "sass": "^1.19.0",
        "sass-loader": "^8.0.0",
        "style-loader": "^0.23.1",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.9.2",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    },
    "engines": {
        "node": ">= 8.0.0",
        "npm": ">= 5.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "resolutions": {
        "graphql": "14.6.0",
        "@apollo/federation": "0.20.7"
    }
}
