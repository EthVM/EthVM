{
  "name": "ethvm-socket-server",
  "version": "0.1.0",
  "description": "Backend socket server for EthVM",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enKryptIO/ethvm-socket-server.git"
  },
  "keywords": [
    "ethvm",
    "socketio",
    "node",
    "socket",
    "ethereum"
  ],
  "author": "EnKryptIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enKryptIO/ethvm-socket-server/issues"
  },
  "homepage": "https://github.com/enKryptIO/ethvm-socket-server#readme",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "nodemon",
    "format": "yarn lint:fix && yarn prettier",
    "lint": "tslint -c tslint.json --project .",
    "lint:fix": "tslint -c tslint.json --project . --fix",
    "test:unit": "jest --config jest.unit.json",
    "test:e2e": "jest --config ./test/jest.e2e.json",
    "test:cov": "jest --coverage --config jest.unit.json",
    "prettier": "prettier --write 'src/**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format"
    }
  },
  "dependencies": {
    "@enkrypt.io/ethereumjs-vm": "^2.3.3",
    "ajv": "^6.5.2",
    "ajv-keywords": "^3.2.0",
    "bignumber.js": "^7.2.1",
    "convict": "^4.3.2",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-account": "^2.0.5",
    "eventemitter3": "^3.1.0",
    "ioredis": "^3.2.2",
    "jayson": "^2.0.6",
    "lodash": "^4.17.10",
    "lru-cache": "^4.1.3",
    "merkle-patricia-tree": "^2.3.1",
    "node-fetch": "^2.2.0",
    "node-rdkafka": "^2.4.1",
    "pino": "^5.0.3",
    "rethinkdb": "^2.3.3",
    "socket.io": "^2.1.1",
    "web3-provider-engine": "^14.0.6",
    "web3-utils": "^1.0.0-beta.35"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/ioredis": "^3.2.14",
    "@types/json-schema": "^7.0.0",
    "@types/lodash": "^4.14.116",
    "@types/lru-cache": "^4.1.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.8",
    "@types/node-fetch": "^2.1.2",
    "@types/pino": "^4.16.1",
    "@types/redis": "^2.8.6",
    "@types/socket.io": "^1.4.36",
    "chai": "^4.1.2",
    "husky": "^1.0.0-rc.9",
    "jest": "^23.5.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "prettier": "1.14.2",
    "shelljs": "^0.8.2",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-consistent-codestyle": "^1.13.3",
    "typescript": "^3.0.1"
  }
}
